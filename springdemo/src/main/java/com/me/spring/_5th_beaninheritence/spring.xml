<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation=  "http://www.springframework.org/schema/beans
                            http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="parentid" class="com.me.spring._5th_beaninheritence.Parent">
        <property name="name" value="parentName"/>
        <property name="behavior" value="behaviorName"/>
    </bean>
    <bean id="childid" class="com.me.spring._5th_beaninheritence.Child" parent="parentid"/>

</beans>